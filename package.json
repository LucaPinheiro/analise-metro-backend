{
    "name": "metro-sp-pipeline-backend",
    "version": "1.0.0",
    "description": "Backend para gerenciamento de pipeline de processamento de imagens e comparação BIM",
    "main": "dist/index.js",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "type-check": "tsc --noEmit",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "seed": "tsx prisma/seed.ts",
        "seed:reset": "tsx prisma/seed.ts --reset",
        "create-project": "tsx scripts/create-project-manual.ts",
        "clear-db": "tsx scripts/clear-db.ts"
    },
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    },
    "keywords": [
        "metro-sp",
        "bim",
        "computer-vision",
        "pipeline"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^6.19.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "multer": "2.0.2",
        "pg": "^8.16.3",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.10.5",
        "@types/pg": "^8.15.6",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "prettier": "3.6.2",
        "prisma": "^6.19.0",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3"
    }
}
